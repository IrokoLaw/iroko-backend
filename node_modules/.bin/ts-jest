#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules/ts-jest/node_modules:/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules:/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules/ts-jest/node_modules:/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules:/home/ousmani/iroko/iroko-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_0a39d2bbc1f2428b66883b5ea4f145e2/node_modules/ts-jest/cli.js" "$@"
fi
